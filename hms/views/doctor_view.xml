    <odoo>
        <data>

            <!--        Doctor Tree View-->
            <record model="ir.ui.view" id="hms_doctor_tree_view">
                <field name="name">HMS doctor Tree</field>
                <field name="model">hms.doctor</field>
                <field name="arch" type="xml">
                    <tree>
                        <field name="first_name"/>
                        <field name="last_name"/>
                    </tree>
                </field>
            </record>

            <!--        Doctor Form View-->
            <record model="ir.ui.view" id="hms_doctor_form_view">
                <field name="name">HMS Doctor Form</field>
                <field name="model">hms.doctor</field>
                <field name="arch" type="xml">
                    <form>
                        <sheet>
                            <field name="image" widget="image" class="oe_avatar"/>
                            <group>
                                <group>
                                    <field name="first_name"/>
                                    <field name="last_name"/>
                                </group>
                            </group>

                        </sheet>
                    </form>
                </field>
            </record>


            <!-- Patient View Action -->

            <record model="ir.actions.act_window" id="hms_doctor_action">
                <field name="name">Doctors List</field>
                <field name="res_model">hms.doctor</field>
                <field name="view_mode">tree,form</field>
            </record>

            <menuitem
                    name="Doctors List"
                    id="doctors_sub_menu1"
                    parent="doctor_menu_item"
                    sequence="1"
                    action="hms_doctor_action"/>


        </data>
    </odoo>